{
    "env": {
      "pathfinderIncludePath": ["${workspaceFolder}/include"],
      "libanvilIncludePath": ["${workspaceFolder}/lib/libanvil/include"]
    },
    "configurations": [
      {
        "name": "WSL",
        "intelliSenseMode": "clang-x64",
        "includePath": ["${libanvilIncludePath}"],
        "defines": ["DEBUG"],
        "forcedInclude": [],
        "compilerPath": "/usr/bin/clang",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "browse": {
          "path": ["${workspaceFolder}"],
          "limitSymbolsToIncludedHeaders": true,
          "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
        }
      }
    ],
    "version": 4
  }